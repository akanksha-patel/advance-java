<?xml version="1.0" encoding="ISO-8859-1"?>


<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">

  <display-name>Welcome to Tomcat</display-name>
  <description>
     Welcome to Tomcat
  </description>

  <servlet>
	<servlet-name>reg001</servlet-name>
	<servlet-class>controllers.RegisterServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>reg001</servlet-name>
	<url-pattern>/register.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>showreg002</servlet-name>
	<servlet-class>controllers.ShowRegisterServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>showreg002</servlet-name>
	<url-pattern>/showregister.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>showlog003</servlet-name>
	<servlet-class>controllers.ShowLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>showlog003</servlet-name>
	<url-pattern>/showlogin.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>log004</servlet-name>
	<servlet-class>controllers.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>log004</servlet-name>
	<url-pattern>/login.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>anth005</servlet-name>
	<servlet-class>controllers.AnotherServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>anth005</servlet-name>
	<url-pattern>/another.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>adm006</servlet-name>
	<servlet-class>controllers.ShowAdminServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>adm006</servlet-name>
	<url-pattern>/admin.do</url-pattern>
  </servlet-mapping>


  <servlet>
	<servlet-name>gu007</servlet-name>
	<servlet-class>controllers.ShowGUServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>gu007</servlet-name>
	<url-pattern>/general_user.do</url-pattern>
  </servlet-mapping>










  <session-config>
	<session-timeout>1</session-timeout>
  </session-config>


  <welcome-file-list>
	<welcome-file>home.jsp</welcome-file>
  </welcome-file-list>

</web-app>
